{
  "tabWidth": 2,
  "printWidth": 100,
  "semi": true,
  "arrowParens": "avoid",
  "trailingComma": "none",
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 160
      }
    },
    {
      "files": "*.spec.ts",
      "options": {
        "printWidth": 120
      }
    }
  ]
}
